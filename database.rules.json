{
  "rules": {
    "profiles":{
      "$user_id":{
        ".read": "$user_id===auth.uid",
        ".write":"$user_id===auth.uid"
      }
    },
    "rooms":{
        ".read": "auth!==null",
        ".write":"auth!==null",
          "$user_id":{
        ".read": "auth!==null",
        ".write":"auth!==null"
      }
      },
"messages":{
        ".read": "auth!==null",
        ".write":"auth!==null",
          "$message_id":{
        ".read": "auth!==null",
        ".write":"auth!==null",
          "lastMessage":{
            ".read":true,
              ".write":true
          }
      },
  "status":{
              "$user_id":{
        ".read": "auth!==null",
        ".write":"$user_id===auth.uid"
      }
  }
      },
    ".read": true,
    ".write":true
  }
}